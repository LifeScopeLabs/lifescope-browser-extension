{
  "name": "example",
  "description": "A Vue.js web extension",
  "permissions": [
    "declarativeContent",
    "history",
    "storage",
    "webNavigation"
  ],
  "version": "0.1",
  "manifest_version": 2,
  "icons": {
    "16": "icons/favicon-16x16.png",
    "32": "icons/android-chrome-36x36.png",
    "48": "icons/android-chrome-48x48.png"
  },
  "options_page": "options/options.html",
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
